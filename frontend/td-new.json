{
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "179707674384.dkr.ecr.ap-south-1.amazonaws.com/frontend:fixed2",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "VITE_API_URL",
          "value": "http://bookstore-alb-536860303.ap-south-1.elb.amazonaws.com/api"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/bookstore-frontend",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "family": "bookstore-frontend",
  "executionRoleArn": "arn:aws:iam::179707674384:role/bookstore-task-exec",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
